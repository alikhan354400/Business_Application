    <script>

        var getWebsiteUrl = '@(System.Configuration.ConfigurationManager.AppSettings["WebsiteUrl"].ToString())';

        // ------------------------ li Menues selection ------------------------------
        $(function () {
            $('#ulMenues li').removeClass('active');
            $('#ulMenues li a').removeClass('toggled');
            $('#ulMenues li').each(function () {
                var aa = $(this).find('a').attr('href');
                var url = document.location.href.replace("#", "");
                if (aa != null && aa != "") {
                    if (url == (getWebsiteUrl + aa)) {
                        $(this).closest('ul').attr('style', 'display:block');
                        $(this).find('a').addClass('toggled');
                        $(this).addClass('active');
                    }
                }
            });
        });

        function AlertMsg(title, message, type) {
            //type: 'success'
            //type: 'warning'
            //type: 'danger'
            //type: 'info'

            $.notify({
                title: title,
                message: message
            }, {
                type: type
            });
        }

        $(".AllowDecimal").on("keypress keyup blur", function (event) {
            //this.value = this.value.replace(/[^0-9\.]/g,'');
            $(this).val($(this).val().replace(/[^0-9\.]/g, ''));
            if ((event.which != 46 || $(this).val().indexOf('.') != -1) && (event.which < 48 || event.which > 57)) {
                event.preventDefault();
            }
        });

        $(".OnlyNumbers").on("keypress keyup blur", function (event) {
            $(this).val($(this).val().replace(/[^\d].+/, ""));
            if ((event.which < 48 || event.which > 57)) {
                event.preventDefault();
            }
        });

        $('.datatable').DataTable({
            responsive: true
        });

        $("input:text,form").attr("autocomplete", "off");

        function PrintPopup(src, width, height, title) {
            var iframe = '<iframe id="reportFrame" width="100%" height="' + height + 'px" scrolling="no" frameborder="0" src="' + src + '" allowfullscreen></iframe>';
            $mymodal = $("#PrintModel");
            $mymodal.attr('data-height', height);
            $mymodal.find("h4.modal-title").html(title);
            $mymodal.find("div.modal-body").html(iframe);
            $mymodal.modal("show");
            $('#PrintModel .modal-dialog').css('width', width + 'px');

            //$('#myModal').attr('data-src', src);
            //$('#myModal').attr('data-height', height);
            //$('#myModalLabel').html(title);
            //$('#myModal').modal('show');
            //$('#myModal .modal-dialog').css('width', width + 'px');

        }
